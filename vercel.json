{
    "version": 2,
    "builds": [
        {
            "src": "dist/production/src/server.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["dist/production/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/production/src/server.js"
        }
    ]
}